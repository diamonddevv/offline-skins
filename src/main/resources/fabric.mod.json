{
  "schemaVersion": 1,
  "id": "stylishserverissues",
  "version": "${version}",

  "name": "Stylish Server Issues",
  "description": "load a locally cached skin when you can't connect to the servers",
  "authors": [
    "DiamondDev"
  ],
  "contact": {
    "homepage": "https://diamonddevv.github.io/",
    "sources": "https://github.com/diamonddevv/stylish-server-issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/stylishserverissues/icon.png",

  "environment": "*",

  "entrypoints": {
    "client": [
      "net.diamonddev.stylishserverissues.StylishServerIssues"
    ]
  },

  "mixins": [
    "stylishserverissues.mixins.json"
  ],

  "accessWidener": "stylishserverissues.accesswidener",


  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.19.3",
    "java": ">=17"
  }
}
